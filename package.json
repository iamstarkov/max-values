  "author": "Vladimir Starkov <iamstarkov@gmail.com> (https://iamstarkov.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iamstarkov/max-values/issues"
  },
  "homepage": "https://github.com/iamstarkov/max-values#readme",
  "devDependencies": {
    "assert": "*",
    "babel": "*",
    "coveralls": "*",
    "isparta": "*",
    "mocha": "*",
    "npm-run-all": "*",
    "trash": "*"
  },
  "dependencies": {
    "camelcase": "^1.2.1",
    "ramda": "^0.17.1"
  }
